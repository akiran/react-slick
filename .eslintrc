{
  "rules": {
    "no-extra-parens": 0,
    "react/jsx-uses-vars": 1,
    "strict": 0,
    "no-underscore-dangle": 0,
    "space-infix-ops": 0,
    "no-alert": 0,
    "react/prop-types": 0,
    "react/no-find-dom-node": 0,
    "react/display-name": 0,
    "no-console": 0
  },
  "env": {
    "node": true,
    "browser": true,
    "es6": true,
    "jasmine": true
  },
  "parser": "babel-eslint",
  "plugins": [
      "react",
      "import"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:react/recommended"
  ]
}
